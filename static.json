{
    "root": "dist/",
    "https_only": true,
    "error_page": "index.html",
    "headers": {
        "/*.js*": {
            "Cache-Control": "public, max-age=86400"
        },
        "/*.otf": {
            "Cache-Control": "public, max-age=604800",
            "Access-Control-Allow-Origin": "*"
        },
        "/*.woff2": {
            "Cache-Control": "public, max-age=604800",
            "Access-Control-Allow-Origin": "*"
        },
        "/*.ttf": {
            "Cache-Control": "public, max-age=604800",
            "Access-Control-Allow-Origin": "*"
        }
    }
}
